{
    "myuuid": "{{random.uuid}}",
    "event_time":"{{date.now("YYYY-MM-DD HH:mm:ss")}}",
    "name": "{{name.firstName}} {{name.lastName}}",
    "address": "{{address.streetAddress}}",
    "city": "{{address.city}}",
    "zipcode": "{{address.zipCode}}",
    "country": "{{address.country}}",
    "email": "{{internet.email}}",
    "phone":"{{phone.phoneNumber}}",
    "coffee": "{{random.arrayElement(
        ["pour", "latte", "mocha"]
    )}}",
    "account": "{{finance.account}}",
    "currency": "{{finance.currencyCode}}",
    "num_ordered": {{random.number(
        {
            "min":1,
            "max":10
        }
    )}},
    "ccnum": "{{random.number(
        {
            "min":1111,
            "max":9999
        }
    )}}-{{random.number(
        {
            "min":1111,
            "max":9999
        }
    )}}-{{random.number(
        {
            "min":1111,
            "max":9999
        }
    )}}-{{random.number(
        {
            "min":1111,
            "max":9999
        }
    )}}",
    "ccexpiry": "{{random.arrayElement(
        ["01","02","03","04","05","06","07","08","09","10","11","12"]
    )}}/{{random.number(
        {
            "min":2025,
            "max":2030
        }
    )}}",
    "ccsecurecode": "{{random.number(
        {
            "min": 111,
            "max": 999
        }
    )}}",
    "ip": "{{internet.ip}}",
    "browser": "{{random.arrayElement(
        ["safari","chrome","IE","Firefox","Opera"]
    )}}"
}
